<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta http-equiv="X-UA-Compatible" content="ie=edge">
  <title>Document</title>
  <style>
    .percent {
      width: 210px;
    }
    .num__box {
      display: inline-block;
      width: 20%;
      text-align: right;
      font-size: 70px;
      font-weight: bold;
    }
    .startBt{
      display: inline-block;
      width: 170px;
      border-radius: 10px;
      background: rgb(96, 211, 226);
      color: #fff;
      font-size: 14px;
      border: 0;
      cursor: pointer;
      padding: 15px;
      margin-top: 20px;
    }
  </style>
</head>

<body>

  <div class="percent">
    <span class="one num__box"></span>
    <span class="two num__box"></span>
    <span class="three num__box"></span>
    <span class="num__box">%</span>
  </div>
  <button class="startBt">START</button>
  <script>
    const startBt=document.getElementsByClassName('startBt')[0];
    startBt.addEventListener('click',function(){
      numFunc()
    })

    function numFunc() {

      function numFuncCount() {
        const num1 = document.getElementsByClassName('one')[0];
        const num2 = document.getElementsByClassName('two')[0];
        const num3 = document.getElementsByClassName('three')[0];

        const numArr = ['1', '2', '3', '4', '5', '6', '7', '8', '9', '$', '%', '&', '#', '@', '!'];

        let numIdx = 0;
        setTimeout(() => {
          numIdx++;
        }, 1000);

        function rancountFunc(t,s)
        {
          const numCount = setInterval(function () {
          const ranNum = Math.floor(Math.random() * numArr.length);
          t.innerHTML = numArr[ranNum];
          if (numIdx === 1) {
            t.innerHTML = s;
            clearInterval(numCount)
          }
        }, 10)
        }
        rancountFunc(num1,'')
        rancountFunc(num2,7)
        rancountFunc(num3,9)
      }
      return {
        numFun: numFuncCount()
      }
    }
    numFunc()
  </script>
</body>

</html>